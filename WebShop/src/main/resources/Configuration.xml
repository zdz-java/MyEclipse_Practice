<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		 <typeAlias alias="Admin" type="com.zdz.model.Admin"/>
		 <typeAlias alias="Member" type="com.zdz.model.Member"/>
		 <typeAlias alias="Cart" type="com.zdz.model.Cart"/>
		 <typeAlias alias="Cartselectedmer" type="com.zdz.model.Cartselectedmer"/>
		 <typeAlias alias="Memberlevel" type="com.zdz.model.Memberlevel"/>
		 <typeAlias alias="Member" type="com.zdz.model.Member"/>
		 <typeAlias alias="Merchandise" type="com.zdz.model.Merchandise"/>
		 <typeAlias alias="Category" type="com.zdz.model.Category"/>
		 <typeAlias alias="Orders" type="com.zdz.model.Orders"/>
		 <typeAlias alias="Leaveword" type="com.zdz.model.Leaveword"/>
	</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="com.mysql.jdbc.Driver"/>
        <property name="url" value="jdbc:mysql://localhost:3306/db_eshop"/>
        <property name="username" value="root"/>
        <property name="password" value="root"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/zdz/mapper/AdminMapper.xml"/>
    <mapper resource="com/zdz/mapper/CartMapper.xml"/>
    <mapper resource="com/zdz/mapper/MemMapper.xml"/>
    <mapper resource="com/zdz/mapper/MerMapper.xml"/>
    <mapper resource="com/zdz/mapper/OrderMapper.xml"/>
    <mapper resource="com/zdz/mapper/WordMapper.xml"/>
  </mappers>
</configuration>